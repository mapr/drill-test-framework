select time_by_day.the_year as c0, employee.management_role as c1, count(salary.employee_id) as m0 from time_by_day as time_by_day, salary as salary, employee as employee where salary.pay_date = time_by_day.the_date and time_by_day.the_year = 1997 and salary.employee_id = employee.employee_id group by time_by_day.the_year, employee.management_role;
