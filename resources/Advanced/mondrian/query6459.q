select promotion.promotion_name as c0, customer.state_province as c1, customer.city as c2, sum(sales_fact_1997.unit_sales) as m0 from promotion as promotion, sales_fact_1997 as sales_fact_1997, customer as customer where sales_fact_1997.promotion_id = promotion.promotion_id and promotion.promotion_name = 'Bag Stuffers' and sales_fact_1997.customer_id = customer.customer_id and customer.state_province in ('CA', 'OR', 'WA') group by promotion.promotion_name, customer.state_province, customer.city;
