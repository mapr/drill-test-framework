select product_class.product_family as c0, product_class.product_department as c1, product_class.product_category as c2, product_class.product_subcategory as c3, store.store_id as c4 from product as product, store as store, product_class as product_class where store.region_id = product_class.product_class_id and product.product_class_id = store.store_id group by product_class.product_family, product_class.product_department, product_class.product_category, product_class.product_subcategory, store.store_id order by product_class.product_family ASC NULLS LAST, product_class.product_department ASC NULLS LAST, product_class.product_category ASC NULLS LAST, product_class.product_subcategory ASC NULLS LAST, store.store_id ASC NULLS LAST;
