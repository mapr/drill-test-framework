select employee_manager_1.management_role as c0, employee_manager_1.position_title as c1, store_1.store_type as c2, sum(inventory.warehouse_sales) as m0 from employee as employee_manager_1, employee as employee_1, inventory_fact_1997 as inventory, store as store_1 where inventory.time_id = employee_1.employee_id and employee_1.supervisor_id = employee_manager_1.employee_id and employee_manager_1.management_role = 'Store Management' and employee_manager_1.position_title = 'Store Manager' and inventory.store_id = store_1.store_id and store_1.store_type in ('Deluxe Supermarket', 'Gourmet Supermarket', 'Supermarket') group by employee_manager_1.management_role, employee_manager_1.position_title, store_1.store_type;
