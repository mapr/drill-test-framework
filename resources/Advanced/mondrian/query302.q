select store.store_country as c0, time_by_day.the_year as c1, sum(inventory_fact_1997.store_invoice) as m0, sum(inventory_fact_1997.supply_time) as m1 from store as store, inventory_fact_1997 as inventory_fact_1997, time_by_day as time_by_day where inventory_fact_1997.store_id = store.store_id and store.store_country = 'USA' and inventory_fact_1997.time_id = time_by_day.time_id and time_by_day.the_year = 1997 group by store.store_country, time_by_day.the_year;
