select customer.marital_status as c0 from customer as customer, sales_fact_1997 as sales_fact_1997, store as store where sales_fact_1997.customer_id = customer.customer_id and sales_fact_1997.store_id = store.store_id and (store.store_state = 'CA') and (customer.gender in ('M', 'F')) group by customer.marital_status order by customer.marital_status ASC NULLS LAST;
