select customer.gender as c0, store.store_country as c1, product_class.product_family as c2 from customer as customer, sales_fact_1997 as sales_fact_1997, store as store, product as product, product_class as product_class where sales_fact_1997.customer_id = customer.customer_id and sales_fact_1997.store_id = store.store_id and product.product_class_id = product_class.product_class_id and sales_fact_1997.product_id = product.product_id and (customer.gender in ('F', 'M')) and (store.store_country = 'USA') and (product_class.product_family in ('Drink', 'Food', 'Non-Consumable')) group by customer.gender, store.store_country, product_class.product_family order by customer.gender ASC NULLS LAST, store.store_country ASC NULLS LAST, product_class.product_family ASC NULLS LAST;
