select product_class.product_family as c0, customer.education as c1 from product as product, product_class as product_class, sales_fact_1997 as sales_fact_1997, customer as customer where product.product_class_id = product_class.product_class_id and sales_fact_1997.product_id = product.product_id and sales_fact_1997.customer_id = customer.customer_id group by product_class.product_family, customer.education order by product_class.product_family ASC NULLS LAST, customer.education ASC NULLS LAST;
