select store.store_name as c0, time_by_day.the_year as c1, count(sales_fact_1997.product_id) as m0 from store as store, sales_fact_1997 as sales_fact_1997, time_by_day as time_by_day where sales_fact_1997.store_id = store.store_id and store.store_name in ('Store 11', 'Store 13', 'Store 15', 'Store 16', 'Store 17', 'Store 2', 'Store 22', 'Store 23', 'Store 3') and sales_fact_1997.time_id = time_by_day.time_id and time_by_day.the_year = 1997 group by store.store_name, time_by_day.the_year;
