select store.store_state as c0, store.store_city as c1, time_by_day.the_year as c2, sum(inventory_fact_1997.warehouse_sales) as m0 from store as store, inventory_fact_1997 as inventory_fact_1997, time_by_day as time_by_day where inventory_fact_1997.store_id = store.store_id and store.store_state in ('OR', 'WA') and store.store_city in ('Bellingham', 'Bremerton', 'Portland', 'Seattle', 'Spokane', 'Tacoma', 'Walla Walla', 'Yakima') and inventory_fact_1997.time_id = time_by_day.time_id and time_by_day.the_year = 1997 group by store.store_state, store.store_city, time_by_day.the_year;
