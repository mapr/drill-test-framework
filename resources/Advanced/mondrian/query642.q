select store.store_name as c0, time_by_day.the_year as c1, time_by_day.week_of_year as c2, time_by_day.day_of_month as c3, product.product_name as c4, warehouse.warehouse_name as c5, sum(inventory_fact_1997.units_shipped) as m0 from store as store, inventory_fact_1997 as inventory_fact_1997, time_by_day as time_by_day, product as product, warehouse as warehouse where inventory_fact_1997.store_id = store.store_id and store.store_name = 'Store 15' and inventory_fact_1997.time_id = time_by_day.time_id and time_by_day.the_year = 1997 and time_by_day.week_of_year = 24 and time_by_day.day_of_month = 3 and inventory_fact_1997.product_id = product.product_id and product.product_name = 'Tell Tale Tomatos' and inventory_fact_1997.warehouse_id = warehouse.warehouse_id and warehouse.warehouse_name = 'Quality Warehousing and Trucking' group by store.store_name, time_by_day.the_year, time_by_day.week_of_year, time_by_day.day_of_month, product.product_name, warehouse.warehouse_name;
