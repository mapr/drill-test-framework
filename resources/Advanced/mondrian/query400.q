select time_by_day.the_year as c0, time_by_day.quarter as c1, time_by_day.month_of_year as c2, customer.state_province as c3, customer.city as c4, customer.gender as c5, sum(sales_fact_1997.unit_sales) as m0 from time_by_day as time_by_day, sales_fact_1997 as sales_fact_1997, customer as customer where sales_fact_1997.time_id = time_by_day.time_id and time_by_day.the_year = 1997 and time_by_day.quarter = 'Q4' and time_by_day.month_of_year = 12 and sales_fact_1997.customer_id = customer.customer_id and customer.state_province = 'OR' and customer.city in ('Albany', 'Beaverton', 'Corvallis', 'Lake Oswego', 'Lebanon', 'Milwaukie', 'Oregon City', 'Portland', 'Salem', 'W. Linn', 'Woodburn') group by time_by_day.the_year, time_by_day.quarter, time_by_day.month_of_year, customer.state_province, customer.city, customer.gender;
