select time_by_day.the_year as c0, time_by_day.quarter as c1, customer.country as c2, customer.state_province as c3 from time_by_day as time_by_day, sales_fact_1997 as sales_fact_1997, customer as customer where sales_fact_1997.time_id = time_by_day.time_id and sales_fact_1997.customer_id = customer.customer_id and (time_by_day.quarter = 'Q1' and time_by_day.the_year = 1997) and (customer.country = 'USA') group by time_by_day.the_year, time_by_day.quarter, customer.country, customer.state_province order by time_by_day.the_year ASC NULLS LAST, time_by_day.quarter ASC NULLS LAST, customer.country ASC NULLS LAST, customer.state_province ASC NULLS LAST;
