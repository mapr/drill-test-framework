select time_by_day.the_year as c0, product_class.product_family as c1, product_class.product_department as c2, product_class.product_category as c3, product_class.product_subcategory as c4, product.brand_name as c5, customer.gender as c6, sum(sales_fact_1997.store_sales) as m0 from time_by_day as time_by_day, sales_fact_1997 as sales_fact_1997, product_class as product_class, product as product, customer as customer where sales_fact_1997.time_id = time_by_day.time_id and time_by_day.the_year = 1997 and sales_fact_1997.product_id = product.product_id and product.product_class_id = product_class.product_class_id and product_class.product_department in ('Carousel', 'Checkout', 'Health and Hygiene', 'Household', 'Periodicals', 'Snacks', 'Starchy Foods') and product_class.product_category in ('Bathroom Products', 'Candles', 'Candy', 'Cleaning Supplies', 'Cold Remedies', 'Decongestants', 'Electrical', 'Hardware', 'Hygiene', 'Kitchen Products', 'Magazines', 'Miscellaneous', 'Pain Relievers', 'Paper Products', 'Plastic Products', 'Specialty', 'Starchy Foods') and product_class.product_subcategory in ('Acetominifen', 'Aspirin', 'Auto Magazines', 'Batteries', 'Candles', 'Cleaners', 'Cold Remedies', 'Computer Magazines', 'Conditioner', 'Deodorizers', 'Fashion Magazines', 'Gum', 'Hard Candy', 'Home Magazines', 'Ibuprofen', 'Lightbulbs', 'Maps', 'Mouthwash', 'Nasal Sprays', 'Paper Dishes', 'Paper Wipes', 'Pasta', 'Personal Hygiene', 'Plastic Utensils', 'Pot Cleaners', 'Pot Scrubbers', 'Pots and Pans', 'Rice', 'Screwdrivers', 'Shampoo', 'Sponges', 'Sports Magazines', 'Sunglasses', 'Toilet Brushes', 'Tools', 'Toothbrushes') and product.brand_name in ('ADJ', 'Akron', 'Atomic', 'Bird Call', 'Black Tie', 'Choice', 'Colossal', 'Consolidated', 'Cormorant', 'Denny', 'Discover', 'Dollar', 'Excel', 'Faux Products', 'Framton', 'Gauss', 'Gulf Coast', 'High Quality', 'Hilltop', 'James Bay', 'Jardon', 'King', 'Medalist', 'Mighty Good', 'Monarch', 'Musial', 'Prelude', 'Queen', 'Red Wing', 'Robust', 'Shady Lake', 'Steady', 'Sunset', 'Symphony', 'Thresher', 'Toretti') and sales_fact_1997.customer_id = customer.customer_id group by time_by_day.the_year, product_class.product_family, product_class.product_department, product_class.product_category, product_class.product_subcategory, product.brand_name, customer.gender;
