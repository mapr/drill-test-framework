select time_by_day.the_year as c0, time_by_day.quarter as c1, customer.gender as c2 from time_by_day as time_by_day, sales_fact_1997 as sales_fact_1997, customer as customer where sales_fact_1997.time_id = time_by_day.time_id and sales_fact_1997.customer_id = customer.customer_id and (customer.gender in ('F', 'M')) group by time_by_day.the_year, time_by_day.quarter, customer.gender order by time_by_day.the_year ASC NULLS LAST, time_by_day.quarter ASC NULLS LAST, customer.gender ASC NULLS LAST;
