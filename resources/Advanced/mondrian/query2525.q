select promotion.promotion_name as c0, customer.marital_status as c1 from promotion as promotion, sales_fact_1997 as sales_fact_1997, customer as customer where sales_fact_1997.promotion_id = promotion.promotion_id and sales_fact_1997.customer_id = customer.customer_id group by promotion.promotion_name, customer.marital_status order by promotion.promotion_name ASC NULLS LAST, customer.marital_status ASC NULLS LAST;
