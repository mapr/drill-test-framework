select product_class.product_family as c0, store.store_country as c1 from product as product, product_class as product_class, sales_fact_1997 as sales_fact_1997, store as store where product.product_class_id = product_class.product_class_id and sales_fact_1997.product_id = product.product_id and sales_fact_1997.store_id = store.store_id and (product_class.product_family in ('Drink', 'Food', 'Non-Consumable')) group by product_class.product_family, store.store_country order by product_class.product_family ASC NULLS LAST, store.store_country ASC NULLS LAST;
