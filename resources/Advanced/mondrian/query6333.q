select customer.country as c0, customer.state_province as c1, customer.city as c2, product_class.product_family as c3, product_class.product_department as c4 from customer as customer, sales_fact_1997 as sales_fact_1997, product as product, product_class as product_class, promotion as promotion where sales_fact_1997.customer_id = customer.customer_id and product.product_class_id = product_class.product_class_id and sales_fact_1997.product_id = product.product_id and sales_fact_1997.promotion_id = promotion.promotion_id and promotion.promotion_name = 'Bag Stuffers' and (customer.country = 'USA') and (product_class.product_department in ('Beverages', 'Dairy') and product_class.product_family = 'Drink') group by customer.country, customer.state_province, customer.city, product_class.product_family, product_class.product_department order by customer.country ASC NULLS LAST, customer.state_province ASC NULLS LAST, customer.city ASC NULLS LAST, product_class.product_family ASC NULLS LAST, product_class.product_department ASC NULLS LAST;
