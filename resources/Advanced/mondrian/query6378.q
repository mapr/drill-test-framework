select time_by_day.the_year as c0, time_by_day.quarter as c1, time_by_day.month_of_year as c2, promotion.promotion_name as c3, sum(sales_fact_1997.unit_sales) as m0 from time_by_day as time_by_day, sales_fact_1997 as sales_fact_1997, promotion as promotion where sales_fact_1997.time_id = time_by_day.time_id and time_by_day.the_year = 1997 and time_by_day.quarter = 'Q1' and time_by_day.month_of_year = 1 and sales_fact_1997.promotion_id = promotion.promotion_id and promotion.promotion_name in ('Big Time Discounts', 'Cash Register Lottery', 'Dollar Days', 'Go For It', 'No Promotion', 'Price Destroyers', 'Price Savers', 'Price Slashers', 'Sales Days', 'Shelf Emptiers', 'Super Wallet Savers', 'Three for One', 'Tip Top Savings', 'Two Day Sale') group by time_by_day.the_year, time_by_day.quarter, time_by_day.month_of_year, promotion.promotion_name;
