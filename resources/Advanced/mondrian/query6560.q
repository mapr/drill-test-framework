select customer.country as c0, customer.state_province as c1, customer.city as c2, product_class.product_family as c3 from customer as customer, sales_fact_1997 as sales_fact_1997, product as product, product_class as product_class where sales_fact_1997.customer_id = customer.customer_id and product.product_class_id = product_class.product_class_id and sales_fact_1997.product_id = product.product_id and (customer.state_province = 'CA') and (product_class.product_family = 'Drink') group by customer.country, customer.state_province, customer.city, product_class.product_family order by customer.country ASC NULLS LAST, customer.state_province ASC NULLS LAST, customer.city ASC NULLS LAST, product_class.product_family ASC NULLS LAST;
