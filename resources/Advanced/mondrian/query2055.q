select time_by_day.month_of_year as c0 from time_by_day as time_by_day, sales_fact_1997 as sales_fact_1997 where sales_fact_1997.time_id = time_by_day.time_id and time_by_day.the_year = 1997 and time_by_day.quarter = 'Q2' group by time_by_day.month_of_year order by time_by_day.month_of_year ASC NULLS LAST;
