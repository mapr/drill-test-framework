select time_by_day.the_year as c0, product.product_name as c1, customer.gender as c2, sum(sales_fact_1997.store_cost) as m0, sum(sales_fact_1997.store_sales) as m1 from time_by_day as time_by_day, sales_fact_1997 as sales_fact_1997, product as product, customer as customer where sales_fact_1997.time_id = time_by_day.time_id and time_by_day.the_year = 1997 and sales_fact_1997.product_id = product.product_id and product.product_name in ('BBB Best Decaf Coffee', 'BBB Best French Roast Coffee', 'BBB Best Regular Coffee', 'Booker 1% Milk', 'Excellent Berry Juice', 'Excellent Cola', 'Excellent Cream Soda', 'Fabulous Apple Juice', 'Fabulous Berry Juice', 'Pearl Chablis Wine', 'Pearl Chardonnay', 'Pearl Chardonnay Wine', 'Pearl Light Beer', 'Pearl Merlot Wine', 'Portsmouth Chardonnay Wine', 'Token Berry Juice', 'Token Diet Cola', 'Top Measure Light Wine', 'Walrus Chablis Wine', 'Walrus Chardonnay Wine', 'Washington Apple Drink', 'Washington Orange Juice') and sales_fact_1997.customer_id = customer.customer_id and customer.gender = 'F' group by time_by_day.the_year, product.product_name, customer.gender;
