select customer.marital_status as c0 from customer as customer, sales_fact_1997 as sales_fact_1997, time_by_day as time_by_day where sales_fact_1997.customer_id = customer.customer_id and sales_fact_1997.time_id = time_by_day.time_id and time_by_day.the_year = 1997 and customer.gender = 'M' group by customer.marital_status having (count(distinct sales_fact_1997.customer_id) > 1380) order by customer.marital_status ASC NULLS LAST;
