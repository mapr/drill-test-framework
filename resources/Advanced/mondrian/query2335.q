select store.store_state as c0, store.store_city as c1, time_by_day.the_year as c2, time_by_day.quarter as c3, time_by_day.month_of_year as c4, product.product_name as c5, sum(sales_fact_1997.unit_sales) as m0 from store as store, sales_fact_1997 as sales_fact_1997, time_by_day as time_by_day, product as product where sales_fact_1997.store_id = store.store_id and store.store_state = 'WA' and store.store_city in ('Bellingham', 'Bremerton', 'Seattle', 'Spokane', 'Tacoma', 'Walla Walla', 'Yakima') and sales_fact_1997.time_id = time_by_day.time_id and time_by_day.the_year = 1997 and time_by_day.quarter = 'Q4' and time_by_day.month_of_year = 12 and sales_fact_1997.product_id = product.product_id and product.product_name = 'Portsmouth Imported Beer' group by store.store_state, store.store_city, time_by_day.the_year, time_by_day.quarter, time_by_day.month_of_year, product.product_name;
