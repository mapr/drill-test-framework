select product_class.product_family as c0, product_class.product_department as c1, product_class.product_category as c2, product_class.product_subcategory as c3, product.brand_name as c4, product.product_name as c5 from product as product, product_class as product_class where product.product_class_id = product_class.product_class_id and (product.brand_name in ('Good', 'Pearl', 'Portsmouth', 'Top Measure', 'Walrus', 'Good', 'Pearl', 'Portsmouth', 'Top Measure', 'Walrus', 'Excellent', 'Fabulous', 'Skinner', 'Token', 'Washington', 'Excellent', 'Fabulous', 'Skinner', 'Token', 'Washington', 'BBB Best', 'CDR', 'Landslide', 'Plato', 'Super', 'BBB Best', 'CDR', 'Landslide', 'Plato', 'Super', 'Excellent', 'Fabulous', 'Skinner', 'Token', 'Washington', 'Booker', 'Carlson', 'Club', 'Even Better', 'Gorilla') and product_class.product_subcategory in ('Beer', 'Wine', 'Soda', 'Flavored Drinks', 'Chocolate', 'Coffee', 'Juice', 'Milk') and product_class.product_category in ('Beer and Wine', 'Carbonated Beverages', 'Drinks', 'Hot Beverages', 'Pure Juice Beverages', 'Dairy') and product_class.product_department in ('Alcoholic Beverages', 'Beverages', 'Dairy') and product_class.product_family = 'Drink') group by product_class.product_family, product_class.product_department, product_class.product_category, product_class.product_subcategory, product.brand_name, product.product_name order by product_class.product_family ASC NULLS LAST, product_class.product_department ASC NULLS LAST, product_class.product_category ASC NULLS LAST, product_class.product_subcategory ASC NULLS LAST, product.brand_name ASC NULLS LAST, product.product_name ASC NULLS LAST;
