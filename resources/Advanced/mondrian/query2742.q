select time_by_day.the_year as c0, department.department_id as c1, employee_closure.supervisor_id as c2, sum(salary.salary_paid) as m0 from time_by_day as time_by_day, salary as salary, department as department, employee_closure as employee_closure where salary.pay_date = time_by_day.the_date and time_by_day.the_year = 1998 and salary.department_id = department.department_id and department.department_id = 14 and salary.employee_id = employee_closure.employee_id and employee_closure.supervisor_id in (366, 367, 368) group by time_by_day.the_year, department.department_id, employee_closure.supervisor_id;
