select store.store_country as c0, store.store_state as c1, customer.education as c2 from store as store, sales_fact_1997 as sales_fact_1997, customer as customer where sales_fact_1997.store_id = store.store_id and sales_fact_1997.customer_id = customer.customer_id and (store.store_state in ('CA', 'OR', 'WA')) group by store.store_country, store.store_state, customer.education order by store.store_country ASC NULLS LAST, store.store_state ASC NULLS LAST, customer.education ASC NULLS LAST;
