select customer.country as c0, customer.state_province as c1 from customer as customer, sales_fact_1997 as sales_fact_1997, time_by_day as time_by_day where sales_fact_1997.customer_id = customer.customer_id and sales_fact_1997.time_id = time_by_day.time_id and time_by_day.the_year = 1997 and (customer.country = 'USA') group by customer.country, customer.state_province having (((sum(sales_fact_1997.store_sales) - sum(sales_fact_1997.store_cost)) > 90000))  order by customer.country ASC NULLS LAST, customer.state_province ASC NULLS LAST;
