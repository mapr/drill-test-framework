select employee_manager_1.management_role as c0, store_1.store_type as c1, sum(inventory.warehouse_sales) as m0, sum(inventory.warehouse_cost) as m1 from employee as employee_manager_1, employee as employee_1, inventory_fact_1997 as inventory, store as store_1 where inventory.time_id = employee_1.employee_id and employee_1.supervisor_id = employee_manager_1.employee_id and employee_manager_1.management_role = 'Store Management' and inventory.store_id = store_1.store_id group by employee_manager_1.management_role, store_1.store_type;
