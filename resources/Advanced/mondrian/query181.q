select time_by_day.the_year as c0, warehouse.warehouse_country as c1, sum(inventory_fact_1997.store_invoice) as m0, sum(inventory_fact_1997.supply_time) as m1 from time_by_day as time_by_day, inventory_fact_1997 as inventory_fact_1997, warehouse as warehouse where inventory_fact_1997.time_id = time_by_day.time_id and inventory_fact_1997.warehouse_id = warehouse.warehouse_id and warehouse.warehouse_country = 'USA' group by time_by_day.the_year, warehouse.warehouse_country;
