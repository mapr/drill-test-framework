

if [[ $# -ne 3 ]]
then
  echo checkcmzhealth needs cluster, errors and timeout
  exit 1
fi

cluster=$1
errors=$2
timeout=$3
echo checkcmzhealth cluster $cluster errors $errors timeout $timeout

# kubectl exec -i -n $cluster admincli-0 -- bash -c 'echo mapr | maprlogin password -user mapr'
./get_mapr_ticket_for_pod $cluster admincli

./checkcldbvalid $cluster $errors $timeout

checkcldbconnectzk $cluster $errors $timeout

./checkcldbmaster $cluster $errors $timeout

./checkzkmaster $cluster $errors $timeout
