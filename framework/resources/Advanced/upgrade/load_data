set -x

# cd framework/resources/Datasources/upgrade

if [[ $# -ne 1 ]]
then
  echo load_data needs the cluster name
  exit 1
fi

cluster=$1
echo load_data cluster $cluster

kubectl cp ../../../resources $cluster/admincli-0:/tmp
kubectl cp copyscript $cluster/admincli-0:/tmp
./get_mapr_ticket_for_pod $cluster admincli
kubectl exec -i admincli-0 -n $cluster -- bash -c "/tmp/copyscript"
