{
    "testId": "advanced_hive",
    "type": "group",
    "description": "Long running hive queries",
    "categories": [
        "smoke"
    ],
    "dependencies" : ["hive"],
    "matrices": [
        {
            "query-file": ".*.q",
            "schema": "hive",
            "output-format": "tsv",
            "expected-file": ".*.e",
            "verification-type": [
                "in-memory"
            ]
        }
    ],
    "datasources": [
    {
        "mode": "cp",
        "src": "Datasources/hive_storage/md1119_parquet",
        "dest": "md1119_parquet"
    },
    {
       "mode": "gen",
       "src": "Datasources/hive_storage/execHive.sh framework/resources/Datasources/hive_storage/md1119.ddl",
       "dest": ""
    }
   ]
}
