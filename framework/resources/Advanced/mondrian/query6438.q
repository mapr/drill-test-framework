select customer.education as c0 from customer as customer, sales_fact_1997 as sales_fact_1997, store as store where sales_fact_1997.customer_id = customer.customer_id and sales_fact_1997.store_id = store.store_id and (store.store_name in ('Store 6', 'Store 2')) group by customer.education order by customer.education ASC NULLS LAST;
