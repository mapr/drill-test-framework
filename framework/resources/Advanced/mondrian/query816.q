select customer.country as c0, customer.state_province as c1, product_class.product_family as c2, customer.gender as c3 from customer as customer, sales_fact_1997 as sales_fact_1997, product as product, product_class as product_class where sales_fact_1997.customer_id = customer.customer_id and product.product_class_id = product_class.product_class_id and sales_fact_1997.product_id = product.product_id group by customer.country, customer.state_province, product_class.product_family, customer.gender order by customer.country ASC NULLS LAST, customer.state_province ASC NULLS LAST, product_class.product_family ASC NULLS LAST, customer.gender ASC NULLS LAST;
