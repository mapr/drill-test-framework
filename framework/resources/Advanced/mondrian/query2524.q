select customer.education as c0, product_class.product_family as c1, store.store_country as c2, store.store_state as c3 from customer as customer, sales_fact_1997 as sales_fact_1997, product as product, product_class as product_class, store as store where sales_fact_1997.customer_id = customer.customer_id and product.product_class_id = product_class.product_class_id and sales_fact_1997.product_id = product.product_id and sales_fact_1997.store_id = store.store_id group by customer.education, product_class.product_family, store.store_country, store.store_state order by customer.education ASC NULLS LAST, product_class.product_family ASC NULLS LAST, store.store_country ASC NULLS LAST, store.store_state ASC NULLS LAST;
