select time_by_day.the_year as c0, time_by_day.quarter as c1, time_by_day.month_of_year as c2, product_class.product_family as c3, product_class.product_department as c4, product_class.product_category as c5, store_ragged.store_state as c6, store_ragged.store_city as c7, sum(sales_fact_1997.unit_sales) as m0 from time_by_day as time_by_day, sales_fact_1997 as sales_fact_1997, product_class as product_class, product as product, store_ragged as store_ragged where sales_fact_1997.time_id = time_by_day.time_id and time_by_day.the_year = 1997 and time_by_day.quarter = 'Q1' and time_by_day.month_of_year in (1, 2, 3) and sales_fact_1997.product_id = product.product_id and product.product_class_id = product_class.product_class_id and product_class.product_department in ('Alcoholic Beverages', 'Baked Goods') and product_class.product_category in ('Beer and Wine', 'Bread') and sales_fact_1997.store_id = store_ragged.store_id and store_ragged.store_state = 'CA' and store_ragged.store_city in ('Alameda', 'Beverly Hills', 'Los Angeles', 'San Francisco') group by time_by_day.the_year, time_by_day.quarter, time_by_day.month_of_year, product_class.product_family, product_class.product_department, product_class.product_category, store_ragged.store_state, store_ragged.store_city;
