select customer.gender as c0, customer.education as c1, product_class.product_family as c2 from customer as customer, sales_fact_1997 as sales_fact_1997, product as product, product_class as product_class where sales_fact_1997.customer_id = customer.customer_id and product.product_class_id = product_class.product_class_id and sales_fact_1997.product_id = product.product_id and (customer.gender = 'M') and (product_class.product_family = 'Drink') group by customer.gender, customer.education, product_class.product_family order by customer.gender ASC NULLS LAST, customer.education ASC NULLS LAST, product_class.product_family ASC NULLS LAST;
