select product_class.product_family as c0 from store as store, inventory_fact_1997 as inventory_fact_1997, product as product, product_class as product_class where inventory_fact_1997.store_id = store.store_id and store.store_sqft = 20319 and store.store_type = 'Supermarket' and product.product_class_id = product_class.product_class_id and inventory_fact_1997.product_id = product.product_id group by product_class.product_family order by product_class.product_family ASC NULLS LAST;
