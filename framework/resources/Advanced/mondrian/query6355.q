select product_class.product_family as c0, product_class.product_department as c1, product_class.product_category as c2, product_class.product_subcategory as c3, product.brand_name as c4, customer.country as c5, customer.state_province as c6, customer.city as c7 from product as product, product_class as product_class, sales_fact_1997 as sales_fact_1997, customer as customer, store as store, time_by_day as time_by_day where product.product_class_id = product_class.product_class_id and sales_fact_1997.product_id = product.product_id and sales_fact_1997.customer_id = customer.customer_id and sales_fact_1997.store_id = store.store_id and store.store_name = 'Store 14' and sales_fact_1997.time_id = time_by_day.time_id and time_by_day.the_year = 1997 and time_by_day.quarter = 'Q1' and time_by_day.month_of_year = 1 and (product_class.product_subcategory = 'Wine' and product_class.product_category = 'Beer and Wine' and product_class.product_department = 'Alcoholic Beverages' and product_class.product_family = 'Drink') and (customer.state_province = 'CA') group by product_class.product_family, product_class.product_department, product_class.product_category, product_class.product_subcategory, product.brand_name, customer.country, customer.state_province, customer.city order by product_class.product_family ASC NULLS LAST, product_class.product_department ASC NULLS LAST, product_class.product_category ASC NULLS LAST, product_class.product_subcategory ASC NULLS LAST, product.brand_name ASC NULLS LAST, customer.country ASC NULLS LAST, customer.state_province ASC NULLS LAST, customer.city ASC NULLS LAST;
