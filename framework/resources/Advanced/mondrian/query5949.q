select time_by_day.the_year as c0, count(distinct sales_fact_1997.customer_id) as m0 from time_by_day as time_by_day, sales_fact_1997 as sales_fact_1997, store as store where sales_fact_1997.time_id = time_by_day.time_id and time_by_day.the_year = 1997 and sales_fact_1997.store_id = store.store_id and (store.store_state in ('BC', 'DF', 'Guerrero', 'Jalisco', 'Veracruz', 'Yucatan', 'Zacatecas', 'CA', 'OR') or ((store.store_city = 'Bellingham' and store.store_state = 'WA') or (store.store_city = 'Bremerton' and store.store_state = 'WA') or (store.store_city = 'Seattle' and store.store_state = 'WA') or (store.store_city = 'Spokane' and store.store_state = 'WA') or (store.store_city = 'Tacoma' and store.store_state = 'WA') or (store.store_city = 'Yakima' and store.store_state = 'WA'))) group by time_by_day.the_year;
