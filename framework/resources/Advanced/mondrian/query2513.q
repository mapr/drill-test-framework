select time_by_day.the_year as c0, time_by_day.quarter as c1, time_by_day.month_of_year as c2, warehouse.warehouse_country as c3, sum(inventory_fact_1997.warehouse_sales) as m0 from time_by_day as time_by_day, inventory_fact_1997 as inventory_fact_1997, warehouse as warehouse where inventory_fact_1997.time_id = time_by_day.time_id and time_by_day.the_year = 1997 and inventory_fact_1997.warehouse_id = warehouse.warehouse_id and warehouse.warehouse_country = 'USA' group by time_by_day.the_year, time_by_day.quarter, time_by_day.month_of_year, warehouse.warehouse_country;
