select customer.gender as c0 from time_by_day as time_by_day, sales_fact_1997 as sales_fact_1997, customer as customer where sales_fact_1997.time_id = time_by_day.time_id and time_by_day.the_year = 1997 and time_by_day.week_of_year in (20, 21, 22, 23) and sales_fact_1997.customer_id = customer.customer_id group by customer.gender order by customer.gender ASC NULLS LAST;
