select store.store_state as c0, time_by_day.the_year as c1, customer.marital_status as c2, count(sales_fact_1997.product_id) as m0 from store as store, sales_fact_1997 as sales_fact_1997, time_by_day as time_by_day, customer as customer where sales_fact_1997.store_id = store.store_id and store.store_state in ('CA', 'OR', 'WA') and sales_fact_1997.time_id = time_by_day.time_id and sales_fact_1997.customer_id = customer.customer_id and customer.marital_status = 'M' group by store.store_state, time_by_day.the_year, customer.marital_status;
