select store.store_name as c0, time_by_day.the_year as c1, time_by_day.quarter as c2, time_by_day.month_of_year as c3, product.product_name as c4, promotion.promotion_name as c5, customer.customer_id as c6, sum(sales_fact_1997.store_sales) as m0 from store as store, sales_fact_1997 as sales_fact_1997, time_by_day as time_by_day, product as product, promotion as promotion, customer as customer where sales_fact_1997.store_id = store.store_id and store.store_name = 'Store 14' and sales_fact_1997.time_id = time_by_day.time_id and time_by_day.the_year = 1997 and time_by_day.quarter = 'Q1' and time_by_day.month_of_year = 1 and sales_fact_1997.product_id = product.product_id and product.product_name in ('Fast Avocado Dip', 'High Top Cauliflower', 'High Top Lemons', 'Kiwi Scallops', 'Moms Sliced Turkey', 'Sphinx Bagels') and sales_fact_1997.promotion_id = promotion.promotion_id and promotion.promotion_name in ('No Promotion', 'Shelf Emptiers', 'Tip Top Savings') and sales_fact_1997.customer_id = customer.customer_id and customer.customer_id in (5, 1924, 2274, 7495, 8830, 10197) group by store.store_name, time_by_day.the_year, time_by_day.quarter, time_by_day.month_of_year, product.product_name, promotion.promotion_name, customer.customer_id;
