select time_by_day.the_year as c0, count(distinct salary.employee_id) as m0 from time_by_day as time_by_day, salary as salary, store as store, employee as employee where salary.pay_date = time_by_day.the_date and time_by_day.the_year = 1997 and salary.employee_id = employee.employee_id and employee.store_id = store.store_id and (store.store_state = 'OR' or ((store.store_city = 'Bremerton' and store.store_state = 'WA') or (store.store_city = 'Seattle' and store.store_state = 'WA') or (store.store_city = 'Spokane' and store.store_state = 'WA') or (store.store_city = 'Tacoma' and store.store_state = 'WA') or (store.store_city = 'Walla Walla' and store.store_state = 'WA') or (store.store_city = 'Yakima' and store.store_state = 'WA'))) group by time_by_day.the_year;
