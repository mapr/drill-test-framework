select promotion.promotion_name as c0 from promotion as promotion, sales_fact_1997 as sales_fact_1997, time_by_day as time_by_day where sales_fact_1997.promotion_id = promotion.promotion_id and sales_fact_1997.time_id = time_by_day.time_id and time_by_day.the_year = 1997 group by promotion.promotion_name having NOT((sum(sales_fact_1997.unit_sales) is null))  order by promotion.promotion_name ASC NULLS LAST;
