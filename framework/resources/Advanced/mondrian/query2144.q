select store.store_state as c0, store.store_city as c1, time_by_day.the_year as c2, customer.customer_id as c3, sum(sales_fact_1997.unit_sales) as m0 from store as store, sales_fact_1997 as sales_fact_1997, time_by_day as time_by_day, customer as customer where sales_fact_1997.store_id = store.store_id and store.store_state = 'WA' and store.store_city = 'Seattle' and sales_fact_1997.time_id = time_by_day.time_id and sales_fact_1997.customer_id = customer.customer_id and customer.customer_id in (570, 6442, 7851) group by store.store_state, store.store_city, time_by_day.the_year, customer.customer_id;
