select time_by_day.the_year as c0, product.brand_name as c1, store_1.store_id as c2, sum(sales_fact_1997.unit_sales) as m0 from time_by_day as time_by_day, sales_fact_1997 as sales_fact_1997, product as product, store as store_1 where sales_fact_1997.time_id = time_by_day.time_id and time_by_day.the_year = 1997 and sales_fact_1997.product_id = product.product_id and product.brand_name in ('Amigo', 'Curlew') and product.product_class_id = store_1.store_id and store_1.store_id = 24 group by time_by_day.the_year, product.brand_name, store_1.store_id;
