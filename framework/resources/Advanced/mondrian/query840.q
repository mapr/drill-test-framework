select store.store_city as c0, time_by_day.the_year as c1, sum(sales_fact_1997.unit_sales) as m0 from store as store, sales_fact_1997 as sales_fact_1997, time_by_day as time_by_day where sales_fact_1997.store_id = store.store_id and store.store_city in ('Alameda', 'Beverly Hills', 'Los Angeles', 'San Diego', 'San Francisco') and sales_fact_1997.time_id = time_by_day.time_id and time_by_day.the_year = 1997 group by store.store_city, time_by_day.the_year;
