select time_by_day.the_year as c0, time_by_day.quarter as c1, warehouse.warehouse_name as c2, sum(inventory_fact_1997.warehouse_cost) as m0, sum(inventory_fact_1997.warehouse_sales) as m1 from time_by_day as time_by_day, inventory_fact_1997 as inventory_fact_1997, warehouse as warehouse where inventory_fact_1997.time_id = time_by_day.time_id and time_by_day.the_year = 1997 and time_by_day.quarter = 'Q2' and inventory_fact_1997.warehouse_id = warehouse.warehouse_id and warehouse.warehouse_name in ('Artesia Warehousing, Inc.', 'Destination, Inc.', 'Jorgensen Service Storage', 'Quality Warehousing and Trucking', 'Treehouse Distribution') group by time_by_day.the_year, time_by_day.quarter, warehouse.warehouse_name;
