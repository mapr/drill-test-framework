select store.store_name as c0, time_by_day.the_year as c1, sum(sales_fact_1997.unit_sales) as m0 from store as store, sales_fact_1997 as sales_fact_1997, time_by_day as time_by_day where sales_fact_1997.store_id = store.store_id and store.store_name in ('Store 1', 'Store 10', 'Store 12', 'Store 14', 'Store 18', 'Store 4', 'Store 5', 'Store 7', 'Store 8') and sales_fact_1997.time_id = time_by_day.time_id and time_by_day.the_year = 1997 group by store.store_name, time_by_day.the_year;
