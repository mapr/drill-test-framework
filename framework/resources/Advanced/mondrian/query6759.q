select store.store_country as c0, store.store_state as c1, time_by_day.the_year as c2 from store as store, sales_fact_1997 as sales_fact_1997, time_by_day as time_by_day where sales_fact_1997.store_id = store.store_id and sales_fact_1997.time_id = time_by_day.time_id and (store.store_state = 'CA') group by store.store_country, store.store_state, time_by_day.the_year order by store.store_country ASC NULLS LAST, store.store_state ASC NULLS LAST, time_by_day.the_year ASC NULLS LAST;
