select time_by_day.the_year as c0, warehouse.warehouse_state_province as c1, warehouse.warehouse_city as c2, sum(inventory_fact_1997.store_invoice) as m0, sum(inventory_fact_1997.supply_time) as m1 from time_by_day as time_by_day, inventory_fact_1997 as inventory_fact_1997, warehouse as warehouse where inventory_fact_1997.time_id = time_by_day.time_id and time_by_day.the_year = 1998 and inventory_fact_1997.warehouse_id = warehouse.warehouse_id and warehouse.warehouse_state_province in ('CA', 'OR', 'WA') and warehouse.warehouse_city in ('Bellingham', 'Bremerton', 'Portland', 'Salem', 'San Diego', 'San Francisco', 'Seattle', 'Spokane', 'Tacoma', 'Walla Walla', 'Yakima') group by time_by_day.the_year, warehouse.warehouse_state_province, warehouse.warehouse_city;
