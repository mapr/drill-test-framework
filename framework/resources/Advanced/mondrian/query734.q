select store.store_country as c0, customer.education as c1, product_class.product_family as c2 from store as store, sales_fact_1997 as sales_fact_1997, customer as customer, product as product, product_class as product_class where sales_fact_1997.store_id = store.store_id and sales_fact_1997.customer_id = customer.customer_id and product.product_class_id = product_class.product_class_id and sales_fact_1997.product_id = product.product_id group by store.store_country, customer.education, product_class.product_family order by store.store_country ASC NULLS LAST, customer.education ASC NULLS LAST, product_class.product_family ASC NULLS LAST;
