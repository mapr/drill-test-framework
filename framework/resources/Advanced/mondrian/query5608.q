select time_by_day.the_year as c0, time_by_day.quarter as c1, time_by_day.month_of_year as c2, customer.customer_id as c3, sum(sales_fact_1997.unit_sales) as m0 from time_by_day as time_by_day, sales_fact_1997 as sales_fact_1997, customer as customer where sales_fact_1997.time_id = time_by_day.time_id and time_by_day.the_year = 1997 and time_by_day.quarter = 'Q1' and time_by_day.month_of_year in (2, 3) and sales_fact_1997.customer_id = customer.customer_id and customer.customer_id in (1297, 2570, 2921, 3491, 4021, 4397, 5174, 6153, 6307, 7702) group by time_by_day.the_year, time_by_day.quarter, time_by_day.month_of_year, customer.customer_id;
