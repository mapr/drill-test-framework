select time_by_day.the_year as c0, store.store_name as c1, sum(salary.salary_paid) as m0 from time_by_day as time_by_day, salary as salary, store as store, employee as employee where salary.pay_date = time_by_day.the_date and salary.employee_id = employee.employee_id and employee.store_id = store.store_id group by time_by_day.the_year, store.store_name;
