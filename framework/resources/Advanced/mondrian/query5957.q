select employee_manager.management_role as c0, store.store_type as c1, sum(inventory.warehouse_sales) as m0 from employee as employee_manager, employee as employee, inventory_fact_1997 as inventory, store as store where inventory.product_id = employee.employee_id and employee.supervisor_id = employee_manager.employee_id and employee_manager.management_role in ('Middle Management', 'Senior Management', 'Store Management') and inventory.warehouse_id = store.store_id group by employee_manager.management_role, store.store_type;
