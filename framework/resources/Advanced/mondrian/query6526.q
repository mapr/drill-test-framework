select product_class.product_subcategory as c0 from product_class as product_class where (product_class.product_category = 'Dry Goods' and product_class.product_department = 'Baking Goods' and product_class.product_family = 'Drink') group by product_class.product_subcategory order by product_class.product_subcategory ASC NULLS LAST;
