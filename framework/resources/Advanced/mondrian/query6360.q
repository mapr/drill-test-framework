select customer.gender as c0, customer.marital_status as c1, customer.country as c2, product_class.product_family as c3 from customer as customer, sales_fact_1997 as sales_fact_1997, product as product, product_class as product_class where sales_fact_1997.customer_id = customer.customer_id and product.product_class_id = product_class.product_class_id and sales_fact_1997.product_id = product.product_id group by customer.gender, customer.marital_status, customer.country, product_class.product_family order by customer.gender ASC NULLS LAST, customer.marital_status ASC NULLS LAST, customer.country ASC NULLS LAST, product_class.product_family ASC NULLS LAST;
