select product_class.product_family as c0, customer.country as c1, store.store_country as c2 from product as product, product_class as product_class, sales_fact_1997 as sales_fact_1997, customer as customer, store as store where product.product_class_id = product_class.product_class_id and sales_fact_1997.product_id = product.product_id and sales_fact_1997.customer_id = customer.customer_id and sales_fact_1997.store_id = store.store_id group by product_class.product_family, customer.country, store.store_country order by product_class.product_family ASC NULLS LAST, customer.country ASC NULLS LAST, store.store_country ASC NULLS LAST;
