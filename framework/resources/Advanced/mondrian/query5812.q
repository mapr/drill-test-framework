select store.store_city as c0, time_by_day.the_year as c1, time_by_day.quarter as c2, product.product_name as c3, sum(sales_fact_1997.store_sales) as m0 from store as store, sales_fact_1997 as sales_fact_1997, time_by_day as time_by_day, product as product where sales_fact_1997.store_id = store.store_id and store.store_city in ('San Diego', 'San Francisco') and sales_fact_1997.time_id = time_by_day.time_id and time_by_day.the_year = 1997 and time_by_day.quarter = 'Q1' and sales_fact_1997.product_id = product.product_id and product.product_name in ('Hilltop Mint Mouthwash', 'Red Spade Low Fat Bologna') group by store.store_city, time_by_day.the_year, time_by_day.quarter, product.product_name;
