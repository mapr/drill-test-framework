select time_by_day.the_year as c0, time_by_day.quarter as c1, time_by_day.month_of_year as c2 from time_by_day as time_by_day where (time_by_day.quarter in ('Q2', 'Q3', 'Q4') and time_by_day.the_year = 1997) group by time_by_day.the_year, time_by_day.quarter, time_by_day.month_of_year order by time_by_day.the_year ASC NULLS LAST, time_by_day.quarter ASC NULLS LAST, time_by_day.month_of_year ASC NULLS LAST;
