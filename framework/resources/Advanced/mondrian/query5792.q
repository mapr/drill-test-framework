select time_by_day.the_year as c0, time_by_day.quarter as c1, store.store_type as c2 from time_by_day as time_by_day, sales_fact_1997 as sales_fact_1997, store as store where sales_fact_1997.time_id = time_by_day.time_id and sales_fact_1997.store_id = store.store_id and (time_by_day.quarter in ('Q1', 'Q2') and time_by_day.the_year = 1997) and (store.store_type in ('Supermarket', 'Deluxe Supermarket', 'Gourmet Supermarket')) group by time_by_day.the_year, time_by_day.quarter, store.store_type order by time_by_day.the_year ASC NULLS LAST, time_by_day.quarter ASC NULLS LAST, store.store_type ASC NULLS LAST;
