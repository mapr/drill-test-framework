select time_by_day.the_year as c0, time_by_day.quarter as c1, time_by_day.month_of_year as c2, customer.gender as c3, customer.marital_status as c4, sum(sales_fact_1997.unit_sales) as m0 from time_by_day as time_by_day, sales_fact_1997 as sales_fact_1997, customer as customer where sales_fact_1997.time_id = time_by_day.time_id and time_by_day.the_year = 1997 and time_by_day.quarter = 'Q2' and time_by_day.month_of_year in (4, 5, 6) and sales_fact_1997.customer_id = customer.customer_id and customer.marital_status in ('M', 'S') group by time_by_day.the_year, time_by_day.quarter, time_by_day.month_of_year, customer.gender, customer.marital_status;
