select product_class.product_family as c0, customer.country as c1, customer.state_province as c2, customer.city as c3 from product as product, product_class as product_class, sales_fact_1997 as sales_fact_1997, customer as customer, promotion as promotion where product.product_class_id = product_class.product_class_id and sales_fact_1997.product_id = product.product_id and sales_fact_1997.customer_id = customer.customer_id and sales_fact_1997.promotion_id = promotion.promotion_id and (promotion.media_type in ('Cash Register Handout', 'Sunday Paper', 'Street Handout')) and (customer.city = 'Bellingham' and customer.state_province = 'WA') group by product_class.product_family, customer.country, customer.state_province, customer.city order by product_class.product_family ASC NULLS LAST, customer.country ASC NULLS LAST, customer.state_province ASC NULLS LAST, customer.city ASC NULLS LAST;
