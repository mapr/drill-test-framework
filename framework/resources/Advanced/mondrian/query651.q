select time_by_day.the_year as c0, customer.state_province as c1, customer.city as c2, sum(sales_fact_1997.unit_sales) as m0 from time_by_day as time_by_day, sales_fact_1997 as sales_fact_1997, customer as customer where sales_fact_1997.time_id = time_by_day.time_id and time_by_day.the_year = 1997 and sales_fact_1997.customer_id = customer.customer_id and customer.city in ('Acapulco', 'Altadena', 'Arcadia', 'Bellflower', 'Berkeley', 'Beverly Hills', 'Burbank', 'Burlingame', 'Burnaby', 'Camacho', 'Chula Vista', 'Cliffside', 'Colma', 'Concord', 'Coronado', 'Daly City', 'Downey', 'El Cajon', 'Fremont', 'Glendale', 'Grossmont', 'Guadalajara', 'Haney', 'Hidalgo', 'Imperial Beach', 'La Cruz', 'La Jolla', 'La Mesa', 'Ladner', 'Lakewood', 'Langford', 'Langley', 'Merida', 'Metchosin', 'Mexico City', 'N. Vancouver', 'Newton', 'Oak Bay', 'Orizaba', 'Port Hammond', 'Richmond', 'Royal Oak', 'San Andres', 'Santa Anita', 'Santa Fe', 'Shawnee', 'Sooke', 'Tixapan', 'Tlaxiaco', 'Vancouver', 'Victoria', 'Westminster') group by time_by_day.the_year, customer.state_province, customer.city;
