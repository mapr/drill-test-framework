select store.store_name as c0, time_by_day.the_year as c1, time_by_day.quarter as c2, time_by_day.month_of_year as c3, promotion.promotion_name as c4, sum(sales_fact_1997.store_sales) as m0 from store as store, sales_fact_1997 as sales_fact_1997, time_by_day as time_by_day, promotion as promotion where sales_fact_1997.store_id = store.store_id and store.store_name = 'Store 14' and sales_fact_1997.time_id = time_by_day.time_id and time_by_day.the_year = 1997 and time_by_day.quarter = 'Q1' and time_by_day.month_of_year = 1 and sales_fact_1997.promotion_id = promotion.promotion_id and promotion.promotion_name in ('No Promotion', 'Shelf Emptiers', 'Tip Top Savings') group by store.store_name, time_by_day.the_year, time_by_day.quarter, time_by_day.month_of_year, promotion.promotion_name;
