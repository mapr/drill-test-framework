select max(sub.tinyint_var), max(length(sub.str_var)), max(length(sub.str_null)), min(length(sub.str_empty)), max(length(sub.str_empty_null)), min(length(sub.str_fixed)), max(length(sub.str_fixed_null_empty)) from ( select str_fixed, str_var, str_null, str_empty, str_empty_null, str_var_null_empty, str_fixed_null_empty, tinyint_var from widestrings order by lower(str_var_null_empty)) sub group by sub.str_var_null_empty;
