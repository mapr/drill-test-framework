{
    "testId": "Table_Stats_Query",
    "type": "group",
    "description": "Test queries using stats",
    "categories": [
        "functional"
    ],
    "matrices": [
        {
            "query-file": ".*.sql",
            "schema": "dfs.drillTestDir",
            "output-format": "tsv",
            "expected-file": ".*.plan",
            "verification-type": [
                "regex"
            ]
        }
    ],
    "datasources": [
        {
                "mode": "cp",
                "src": "Datasources/table_stats/tables",
                "dest": "table_stats"
        },
        {
            "mode": "gen",
            "src": "Datasources/table_stats/table_stats.sh",
            "dest": ""
        }
    ]
}
