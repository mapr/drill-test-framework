            "verification-type": [
                "in-memory"
            ]
        }
    ],
    "datasources": [
        {
            "mode": "cp",
            "src": "Datasources/tpcds/sf1/text",
            "dest": "tpcds_sf1/text"
        },
        {
            "mode": "gen",
            "src": "Datasources/hive/execHive.sh framework/resources/Datasources/hive_storage/tpcds01_text.ddl",
            "dest": ""
        }
    ]
}
