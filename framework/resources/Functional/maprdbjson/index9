
set -x

maprcli table index add -path /apps/grades -index idx_studentd -indexedfields 'student_id:desc' -hashed true
maprcli table index list -path /apps/grades -json

echo 'find /apps/grades --query {"$select":["_id","student_id"],"$orderby":{"student_id":"desc"}}' | mapr dbshell

maprcli table index remove -path /apps/grades -index idx_studentd
maprcli table index list -path /apps/grades -json
