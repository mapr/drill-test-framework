set -x

maprcli table index list -path /apps/grades -json
maprcli table index add -path /apps/grades -index idx_student -indexedfields 'student_id:1'
maprcli table index add -path /apps/grades -index idx_studentd -indexedfields 'student_id:desc'
sleep 240

maprcli table index list -path /apps/grades -json

maprcli table index remove -path /apps/grades -index idx_student
maprcli table index remove -path /apps/grades -index idx_studentd
