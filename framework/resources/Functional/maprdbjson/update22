echo 'insert /apps/scores --value '\''{"_id":"abcd","class_id":5,"scores":[{"score":70.1,"type":"quiz"}]}'\' | mapr dbshell
echo 'update /apps/scores --id abcd --m '\''{ "$set":{"scores":[{"score":70.1,"type":"exam"}]}}'\' | mapr dbshell
echo 'find /apps/scores --query {"$select":["_id","class_id","scores"],"$where":{"$eq":{"_id":"abcd"}}}' | mapr dbshell

echo 'delete /apps/scores --id "abcd"' | mapr dbshell
